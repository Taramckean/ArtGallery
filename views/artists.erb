<img src="/freud_page2.png" class="responsive-image">

<a href="/gallery/new_artist">Add new artist</a>

<% for artist in @artists %>
<p>Name: <%= artist.name() %></p>
<p>Year of birth: <%= artist.birth_year() %></p>
<p>Genre: <%= artist.art_movement() %></p>

<a class ="button" href="/artists/<%= @artist%>/edit">Edit</a>

<form action="/artists/<%= artist.id%>/delete">
  <input class= "button" type="submit" value ="Delete"/>
</form>

<% end %>
